     be excuted in parallel. However groups will be executed sequencially
     as defined in tag execution

  <xml>
    <global>
    <store_dir>equities_db/data_20171113/</store_dir>
    <list_db>equities_db/lists/</list_db>
    <verbosity>0</verbosity>
    </global>

    <execution>
      <line>retriver_group</line>
      <line>parser_group</line>
      .
      .
      <line></line>
    </execution>

    <group id="retriver_group" >
      <process></process>
      <process></process>
      .
      .
      <process></process>
    <group>

    <group id="parser_group" >
      <process></process>
      <process></process>
      .
      .
      <process></process>
    <group>

    .
    .
    .
  </xml>


-->

<xml>
  <global>
    <store_dir>equities_db/X/</store_dir>
    <list_db>equities_db/lists/</list_db>
    <!-- <log_dir>equities_db/data_20171113/retriver_</log_dir> -->
    <verbosity>0</verbosity>
  </global>

  <execution>
    <line>retriver</line>
    <line>wsj_parser</line>
    <line>wsj_inserter</line>
  </execution>

  <group id=" retriver">
    <process>
      <type>retriver</type>
      <task>wsj</task>
      <exchange>xhkex</exchange>
    </process>

    <process>
      <type>retriver</type>
      <task>wsj</task>
      <exchange>xnse</exchange>
    </process>

    <process>
      <type>retriver</type>
      <task>wsj</task>
      <exchange>xnyse</exchange>
    </process>

    <process>
      <type>retriver</type>
      <task>wsj</task>
      <exchange>xtyo</exchange>
    </process>
  </group>


  <group id="wsj_parser">
    <process>
      <type>parser</type>
      <task>wsj</task>
      <exchange>xhkex</exchange>
    </process>

    <process>
      <type>parser</type>
      <task>wsj</task>
      <exchange>xnse</exchange>
    </process>

    <process>
      <type>parser</type>
      <task>wsj</task>
      <exchange>xnyse</exchange>
    </process>

    <process>
      <type>parser</type>
      <task>wsj</task>
      <exchange>xtyo</exchange>
    </process>
  </group>

  <group id="wsj_inserter">
    <process>
      <type>inserter</type>
      <exchange>xhkex</exchange>
    </process>

    <process>
      <type>inserter</type>
      <exchange>xnse</exchange>
    </process>

    <process>
      <type>inserter</type>
      <exchange>xnyse</exchange>
    </process>

    <process>
      <type>inserter</type>
      <exchange>xtyo</exchange>
    </process>
  </group>

</xml>
